    "@types/express": "^5.0.6",
    "@types/express-fileupload": "^1.5.1",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash.throttle": "^4.1.9",
    "@types/passport": "^1.0.17",
