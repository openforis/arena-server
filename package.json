  "bugs": {
    "url": "https://github.com/openforis/arena-server/issues"
  },
  "homepage": "https://github.com/openforis/arena-server#readme",
  "packageManager": "yarn@4.12.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
